{
	"appname": "nodea_workspace",
	"version": "1.0.0",
	"build": 0,
	"maintenance": false,
	"api_enabled": true,
	"lang": "fr-FR",
	"timezone": "Europe/Paris",
	"contact_field_for_sms": "f_phone",
	"socket": {
		"enabled": false,
		"chat": false,
		"notification": false
	},
	"document_template": {
		"format_pairs": [{
			"code": "docx_docx",
			"label": "global_component.document_template.format_pairs.docx_docx",
			"input": "docx",
			"output": "docx"
		}, {
			"code": "dust_pdf",
			"label": "global_component.document_template.format_pairs.dust_pdf",
			"input": "dust",
			"output": "pdf"
		}],
		"input_extentions": ["pdf", "dust", "html", "doc", "docx"],
		"output_extentions": ["docx", "pdf", "html"]
	},
	"resizePicture": {
		"enabled": false,
		"width": 400,
		"height": 400,
		"quality": 100
	},
	"thumbnail": {
		"folder": "thumbnail/",
		"width": 30,
		"height": 30,
		"quality": 60
	},
	"dropzoneInfo": {
		"sizeFileLimit": 50000000
	},
	"timepicker": {
		"defaultDate": {
			"dateFormat": "YYYY-MM-DD 00:00"
		}
	},
	"datepicker": {
		"defaultDate": {
			"dateFormat": "YYYY-MM-DD"
		}
	}
}
